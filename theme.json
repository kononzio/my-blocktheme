{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Josefin Sans\"",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/josefin-sans-300-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Josefin Sans\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/josefin-sans-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Josefin Sans\", sans-serif",
					"name": "Josefin Sans",
					"slug": "josefin-sans"
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "13px",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "18px",
						"min": "16px"
					},
					"size": "18px",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "32px",
						"min": "22px"
					},
					"size": "32px",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "64px",
						"min": "36px"
					},
					"size": "64px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "120px",
						"min": "60px"
					},
					"name": "Super Extra Large",
					"size": "120px",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
